{
  "expo": {
    "name": "ARTag",
    "slug": "ARTag",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "artag",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "ARTag would like to read photos for AR experiences.",
        "NSPhotoLibraryAddUsageDescription": "Allow $(PRODUCT_NAME) to save photos",
        "NSCameraUsageDescription": "ARTag uses your camera for AR experiences.",
        "NSMicrophoneUsageDescription": "ARTag uses your microphone for AR experiences."
      },
      "bundleIdentifier": "com.nnadozi.ARTag"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.nnadozi.ARTag"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "@reactvision/react-viro",
        {
          "android": {
            "xRMode": [
              "AR",
              "GVR"
            ]
          },
          "ios": {
            "cameraUsagePermission": "ARTag uses your camera for AR experiences.",
            "microphoneUsagePermission": "ARTag uses your microphone for AR experiences.",
            "photosPermission": "ARTag would like to read photos for AR experiences.",
            "savephotosPermission": "ARTag would like to save photos to your library during AR experiences."
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
